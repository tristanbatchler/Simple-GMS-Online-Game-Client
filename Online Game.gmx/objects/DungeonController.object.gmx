            <string>/// Get list of allies and enemies.

allies = ds_list_create();
enemies = ds_list_create();

nAllies = instance_number(parAlly);
nEnemies = instance_number(parEnemy);

// Get all allies.
for (var i = 0; i &lt; nAllies; i++) {
    ds_list_add(allies, instance_find(parAlly, i));
    }
    
// Get all enemies.
for (var i = 0; i &lt; nEnemies; i++) {
    ds_list_add(enemies, instance_find(parEnemy, i));
    }
    
// Player is now an enemy for some reason, so fix this with CHEESE.
ds_list_delete(enemies, ds_list_find_index(enemies, self));
nEnemies--;
ds_list_delete(allies, ds_list_find_index(allies, self));
nAllies--;
</string>
